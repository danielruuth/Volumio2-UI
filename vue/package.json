{
  "name": "wave-ui",
  "version": "1.0.0",
  "description": "GUI for the Wave project running on Volumio2",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "npx webpack --progress --hide-modules --mode=development --config=webpack.config.js",
    "watch": "npx webpack --watch --progress --hide-modules --mode=development --config=webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "prod": "npm run production",
    "production": "npx webpack --progress --hide-modules --mode=production --config=webpack.config.js"
  },
  "author": "Daniel Ruuth",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "moment": "^2.24.0",
    "momentjs": "^2.0.0",
    "vue": "^2.6.2",
    "vue-router": "^3.0.2",
    "vue-socket.io": "^3.0.5",
    "vuejs": "^3.0.1",
    "webpack-dev-server": "^3.1.14"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.2",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3"
  }
}
